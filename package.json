{
  "name": "fullpack",
  "version": "2.1.0",
  "description": "Normal front-end project template.",
  "engines": {
    "node": ">=6.9.1",
    "npm": ">= 3.10.0"
  },
  "scripts": {
    "release-patch": "npm version patch && git push && git push --tags",
    "release-minor": "npm version minor && git push && git push --tags",
    "release-major": "npm version major && git push && git push --tags"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.34",
    "@babel/polyfill": "7.0.0-beta.34",
    "@babel/preset-env": "7.0.0-beta.34",
    "autoprefixer": "7.2.2",
    "babel-eslint": "8.0.3",
    "babel-loader": "8.0.0-beta.0",
    "cache-loader": "1.2.0",
    "copy-webpack-plugin": "4.2.3",
    "css-loader": "0.28.7",
    "cssnano": "3.10.0",
    "eslint": "4.13.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-friendly-formatter": "3.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.8.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "globby": "7.1.1",
    "handlebars": "4.0.11",
    "handlebars-loader": "1.6.0",
    "html-loader": "0.5.1",
    "html-webpack-plugin": "2.30.1",
    "json-loader": "0.5.7",
    "koa": "2.4.1",
    "koa-compose": "4.0.0",
    "koa-proxies": "0.6.1",
    "koa-static": "4.0.2",
    "loader-utils": "1.1.0",
    "postcss-loader": "2.0.9",
    "postcss-reporter": "5.0.0",
    "precss": "2.0.0",
    "progress-bar-webpack-plugin": "1.10.0",
    "rimraf": "2.6.2",
    "style-loader": "0.19.0",
    "stylelint": "8.3.1",
    "stylelint-config-standard": "18.0.0",
    "uglifyjs-webpack-plugin": "1.1.2",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-dev-middleware": "1.12.2",
    "webpack-hot-middleware": "2.21.0",
    "webpack-merge": "4.1.1"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fbi-templates/fbi-project-fullpack.git"
  },
  "keywords": [
    "front-end",
    "development",
    "frameworkless",
    "handlebars",
    "tasks",
    "fbi-project",
    "fbi"
  ],
  "author": "neikvon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fbi-templates/fbi-project-fullpack/issues"
  },
  "homepage": "https://github.com/fbi-templates/fbi-project-fullpack#readme",
  "fbi": {
    "template": "fbi-project-fullpack",
    "tasks": {
      "build": {
        "alias": "b",
        "desc": "Build the project for the specified environment."
      },
      "serve": {
        "alias": "s",
        "desc": "Start development server. (-p: serve dist folder)"
      },
      "clean": {
        "alias": "c",
        "desc": "Clean up destinations"
      }
    }
  }
}
